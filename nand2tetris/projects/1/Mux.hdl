/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Or(a=partA, b=partB, out=out);
	And(a=a, b=selNot, out=partA);
	And(a=sel, b=b, out=partB);
	Not(in=sel, out=selNot);
}